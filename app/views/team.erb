<h1>Superhero Team</h1>
 
<div class="team">
  <h3>Name: <%= @team.name %></h3>
  <h4>Team Motto: <%= @team.motto %></h4>
</div><br>

<h2>Heroes</h2>
<% @heroes.each do |hero| %>
  <div class="hero">
    <p>Hero Name: <%= hero.name %></p>
    <p>Hero Power: <%= hero.power %></p>
    <p>Hero Biography: <%= hero.biography %></p>
  </div><br>
<% end %>